{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    ".next": true,
    ".vscode": true,
    "tsconfig.json": true,
    "tailwind.config.cjs": true,
    "README.md": true,
    "postcss.config.cjs": true,
